{
    "name": "imemento/health-probe",
    "description": "Health Probe",
    "homepage": "https://github.com/mementohub/health-probe",
    "license": "MIT",
    "authors": [
        {
            "name": "George Stan",
            "email": "contact@georgestan.com",
            "homepage": "http://imemento.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "iMemento\\HealthProbe\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iMemento\\HealthProbe\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "iMemento\\HealthProbe\\ServiceProvider"
            ]
        }
    }
}
