{
    "name": "imemento/health-probe",
    "description": "Health Probe",
    "homepage": "https://gitlab.com/imemento/composer/packages/health-probe",
    "license": "MIT",
    "authors": [
        {
            "name": "George Stan",
            "email": "contact@georgestan.com",
            "homepage": "http://imemento.com"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "iMemento\\HealthProbe\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iMemento\\HealthProbe\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {"type": "composer", "url": "https://repo.packagist.com/imemento/"},
        {"packagist.org": false}
    ],
    "minimum-stability": "dev",
    "prefer-stable": false,
    "extra": {
        "laravel": {
            "providers": [
                "iMemento\\HealthProbe\\ServiceProvider"
            ]
        }
    }
}
